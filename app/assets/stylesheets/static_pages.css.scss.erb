@-webkit-keyframes horizontalslider {
0% { -webkit-transform: translateX(0px); }

20% { -webkit-transform: translateX(0px); }

25% { -webkit-transform: translateX(-1027px); }

45% { -webkit-transform: translateX(-1027px); }

50% { -webkit-transform: translateX(-2051px); }

70% { -webkit-transform: translateX(-2051px); }

75% { -webkit-transform: translateX(-3078px); }

95% { -webkit-transform: translateX(-3078px); }

100% { -webkit-transform: translateX(-4105px);  }


}
	
@-moz-keyframes horizontalslider {

0% { -moz-transform: translateX(0px); }

20% { -moz-transform: translateX(0px); }

25% { -moz-transform: translateX(-1027px); }

45% { -moz-transform: translateX(-1027px); }

50% { -moz-transform: translateX(-2051px); }

70% { -moz-transform: translateX(-2051px); }

75% { -moz-transform: translateX(-3078px); }

95% { -moz-transform: translateX(-3078px); }

100% { -moz-transform: translateX(-4105px);  }


}







